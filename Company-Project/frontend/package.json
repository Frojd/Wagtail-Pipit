{
  "name": "react-sass-starterkit",
  "version": "4.1.0",
  "description": "An opinionated react starterkit",
  "main": "index.js",
  "engines": {
    "node": ">=8.5"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production -p",
    "delete": "node bin/cli.js delete",
    "eslint": "eslint app",
    "eslint:fix": "eslint app --fix",
    "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
    "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
    "multiwatch": "concurrently \"npm run watch\" \"npm run start\"",
    "new": "node bin/cli.js new",
    "scaffold": "node bin/cli.js scaffold",
    "ssr": "cross-env webpack --mode=development --config ./webpack.ssr.config.js",
    "ssr:prod": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack.ssr.config.js -p",
    "ssr:watch": "cross-env NODE_ENV=production webpack --mode=development --config ./webpack.ssr.config.js --watch",
    "start": "node ./bin/dev-server",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:out": "build-storybook -c .storybook -o dist/storybook",
    "test": "jest",
    "watch": "webpack --mode=development --watch"
  },
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/react-sass-starterkit.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "scss",
    "hmr",
    "react",
    "frontend",
    "boilerplate",
    "starterkit",
    "cli",
    "scaffold"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.setup-test.js"
    ],
    "testMatch": [
      "**/?(*.)(test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/internals/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.3",
    "@storybook/addon-actions": "^4.1.13",
    "@storybook/addon-info": "^4.1.13",
    "@storybook/addon-knobs": "^4.1.13",
    "@storybook/addon-options": "^4.1.13",
    "@storybook/react": "^4.1.13",
    "autoprefixer": "^9.4.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-docgen": "^2.0.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "commander": "^2.19.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-scss": "^2.0.0",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.7.1",
    "react-styleguidist": "^9.0.1",
    "sass-loader": "^7.1.0",
    "storybook-addon-smart-knobs": "^4.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "i18next": "^15.0.4",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
