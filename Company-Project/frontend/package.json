{
    "name": "frontend_nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next export",
        "storybook": "start-storybook -p 3001 -s ./public",
        "build-storybook": "npx build-storybook -s public",
        "fixcode": "npx prettier \"**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
        "new": "node cli/bin.js new",
        "new:container": "node cli/bin.js new --container",
        "test": "jest --watch",
        "test:ci": "jest -w 1 --ci"
    },
    "dependencies": {
        "@sentry/browser": "^5.24.2",
        "@sentry/integrations": "^5.24.2",
        "@sentry/node": "^5.24.2",
        "@sentry/webpack-plugin": "^1.13.0",
        "@svgr/webpack": "^5.4.0",
        "@zeit/next-source-maps": "0.0.4-canary.1",
        "i18next": "^19.6.0",
        "next": "10.0.1",
        "next-compose-plugins": "^2.2.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "@types/react": "^16.9.32",
        "classnames": "^2.2.6",
        "prop-types": "^15.7.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern 'frontend/**' && jest --onlyChanged",
            "pre-push": ""
        }
    },
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@storybook/addon-a11y": "^5.3.19",
        "@storybook/addon-actions": "^5.3.19",
        "@storybook/addon-links": "^5.3.19",
        "@storybook/addon-viewport": "^5.3.19",
        "@storybook/addons": "^5.3.19",
        "@storybook/react": "^5.3.19",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "babel-loader": "^8.1.0",
        "cross-env": "^7.0.2",
        "css-loader": "^4.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.4.2",
        "jest-svg-transformer": "^1.0.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "react-test-renderer": "^16.12.0",
        "style-loader": "^1.2.1"
    }
}
