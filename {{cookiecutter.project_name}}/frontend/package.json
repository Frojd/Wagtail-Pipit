{
  "name": "shoggoth",
  "version": "0.0.1",
  "description": "An opinionated react starterkit",
  "main": "index.js",
  "engines": {
    "node": ">=8.5"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production -p",
    "delete": "node bin/cli.js delete",
    "eslint": "eslint app",
    "eslint:fix": "eslint app --fix",
    "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
    "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
    "multiwatch": "concurrently \"npm run watch\" \"npm run start\"",
    "new": "node bin/cli.js new",
    "scaffold": "node bin/cli.js scaffold",
    "ssr": "cross-env webpack --mode=development --config ./webpack.ssr.config.js",
    "ssr:prod": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack.ssr.config.js -p",
    "ssr:watch": "cross-env NODE_ENV=production webpack --mode=development --config ./webpack.ssr.config.js --watch",
    "start": "node ./bin/dev-server",
    "styleguide": "styleguidist server",
    "test": "jest",
    "watch": "webpack --mode=development --watch"
  },
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.setup-test.js"
    ],
    "testMatch": [
      "**/?(*.)(test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/internals/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-webpack-alias": "^2.1.2",
    "commander": "^2.19.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-scss": "^2.0.0",
    "precss": "^4.0.0",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.6.3",
    "react-styleguidist": "^8.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "i18next": "^13.1.1",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
