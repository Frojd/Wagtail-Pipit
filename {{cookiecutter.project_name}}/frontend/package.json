{
    "name": "frontend_nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p {{cookiecutter.nextjs_port}}",
        "build": "next build",
        "build:local": "IGNORE_SENTRY=1 next build",
        "start": "next start -p {{cookiecutter.nextjs_port}}",
        "export": "next export",
        "storybook": "start-storybook -p {{cookiecutter.storybook_port}} -s ./public",
        "build-storybook": "npx build-storybook -s public",
        "fixcode": "npx prettier \"**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
        "new": "node cli/bin.js new",
        "new:container": "node cli/bin.js new --container",
        "test": "jest --watch",
        "test:ci": "npx jest --ci --runInBand --reporters=default --reporters=jest-junit"
    },
    "dependencies": {
        "@sentry/nextjs": "^6.17.1",
        "@types/react": "^17.0.38",
        "classnames": "^2.3.1",
        "i18next": "^21.6.6",
        "next": "12.0.8",
        "next-compose-plugins": "^2.2.1",
        "prop-types": "^15.8.1",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern 'frontend/**' && jest --onlyChanged",
            "pre-push": ""
        }
    },
    "devDependencies": {
        "@babel/core": "^7.16.7",
        "@storybook/addon-a11y": "^6.4.13",
        "@storybook/addon-viewport": "^6.4.13",
        "@storybook/addons": "^6.4.13",
        "@storybook/builder-webpack5": "^6.4.13",
        "@storybook/manager-webpack5": "^6.4.13",
        "@storybook/react": "^6.4.13",
        "@svgr/webpack": "^6.2.0",
        "@swc/core": "^1.2.131",
        "@swc/jest": "^0.2.17",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "babel-loader": "^8.2.3",
        "babel-plugin-transform-dynamic-import": "^2.1.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.4.7",
        "jest-junit": "^13.0.0",
        "jest-next-dynamic": "^1.0.1",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "react-test-renderer": "^17.0.2",
        "style-loader": "^3.3.1"
    }
}
